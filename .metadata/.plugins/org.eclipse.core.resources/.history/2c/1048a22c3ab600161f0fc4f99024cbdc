/**
 * 
 */
package origine;

/**
 * @author Lallement
 *
 */
public class Origine {
	/* ---------- Attributs ---------- */
	private LibelleOrigine origine;
	
	/* ---------- Constructeurs ---------- */
	/**
	 * Constructeur avec argument.
	 * Toute origine nouvellement créée doit posséder un libellé.
	 * @param {LibelleOrigine} libO : libellé de l'origine créée
	 */
	public Origine (LibelleOrigine libO) {
		this.origine = libO;
	}
	
	/* ---------- Getters & Setters ---------- */
	/**
	 * Accesseur permettant d'obtenir le libellé d'une origine
	 * @return {LibelleOrigine} origine : retourne le libellé de l'origine 
	 */
	public LibelleOrigine getOrigine() {
		return origine;
	}
	
	/**
	 * Modificateur permettant de changer le libellé d'une origine.
	 * @param {LibelleOrigine} origine : nouveau libellé
	 */
	public void setOrigine(LibelleOrigine origine) {
		this.origine = origine;
	}
	
	/* ---------- Méthodes ---------- */
	/**
	 * Méthode de tests
	 * @param {String[]} args
	 */
	public static void main(String[] args) {

	}
}
